{
  "name": "exceljs",
  "version": "4.4.0",
  "description": "Excel Workbook Manager - Read and Write xlsx and csv Files.",
  "license": "MIT",
  "contributors": [
    "Guyon Roche <guyon@live.com>",
    "Paweł Dąbrowski <dabrowskip9@gmail.com>"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/paolostyle/exceljs.git"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/exceljs.cjs",
  "module": "./dist/exceljs.js",
  "browser": "./dist/exceljs.js",
  "types": "./index.d.ts",
  "files": [
    "dist",
    "lib",
    "LICENSE",
    "README.md",
    "README_zh.md",
    "index.d.ts"
  ],
  "imports": {
    "#lib": "./src/index.js",
    "#lib/*": "./src/*"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:manual": "node spec/manual/app.js",
    "lint": "biome check",
    "lint:fix": "biome check --fix",
    "benchmark": "node --expose-gc benchmark",
    "benchmark:debug": "node --expose-gc --inspect-brk --trace-deopt benchmark",
    "prepare": "husky",
    "knip": "knip"
  },
  "keywords": [
    "xls",
    "xlsx",
    "csv",
    "excel",
    "office",
    "spreadsheet",
    "workbook",
    "sheets"
  ],
  "dependencies": {
    "archiver": "^5.3.2",
    "dayjs": "^1.11.13",
    "fast-csv": "^5.0.2",
    "jszip": "^3.10.1",
    "readable-stream": "^3.6.0",
    "saxes": "^5.0.1",
    "tmp": "^0.2.3",
    "unzipper": "^0.12.3",
    "web-streams-polyfill": "^4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.13.0",
    "@types/chai-xml": "^0.3.6",
    "@types/chai-datetime": "1.0.0",
    "chai-datetime": "^1.8.1",
    "chai-xml": "^0.4.1",
    "express": "^4.16.4",
    "got": "^9.0.0",
    "husky": "^9.1.7",
    "knip": "^5.43.6",
    "lint-staged": "^15.4.3",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  },
  "lint-staged": {
    "*": [
      "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "packageManager": "pnpm@10.1.0+sha512.c89847b0667ddab50396bbbd008a2a43cf3b581efd59cf5d9aa8923ea1fb4b8106c041d540d08acb095037594d73ebc51e1ec89ee40c88b30b8a66c0fae0ac1b",
  "pnpm": {
    "onlyBuiltDependencies": [
      "husky",
      "esbuild",
      "@biomejs/biome"
    ]
  },
  "cjs2esm": {
    "input": [
      "lib"
    ]
  }
}
