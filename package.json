{
  "name": "exceljs",
  "version": "4.4.0",
  "description": "Excel Workbook Manager - Read and Write xlsx and csv Files.",
  "license": "MIT",
  "contributors": [
    "Guyon Roche <guyon@live.com>",
    "Paweł Dąbrowski <dabrowskip9@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/paolostyle/exceljs.git"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.mjs",
  "types": "./index.d.ts",
  "files": [
    "dist",
    "lib",
    "LICENSE",
    "README.md",
    "README_zh.md",
    "index.d.ts"
  ],
  "imports": {
    "#lib": "./lib/index.js",
    "#lib/*": "./lib/*"
  },
  "scripts": {
    "test": "npm run build && npm run test:unit && npm run test:integration && npm run test:end-to-end",
    "test:unit": "mocha --require spec/config/setup spec/unit --recursive",
    "test:integration": "mocha --require spec/config/setup spec/integration --recursive",
    "test:end-to-end": "mocha --require spec/config/setup spec/end-to-end --recursive",
    "test:manual": "node spec/manual/app.js",
    "lint": "eslint --format node_modules/eslint-friendly-formatter .",
    "lint:fix": "prettier-eslint --write $(pwd)'/**/*.js'",
    "benchmark": "node --expose-gc benchmark",
    "benchmark:debug": "node --expose-gc --inspect-brk --trace-deopt benchmark",
    "build": "tsup",
    "postversion": "git push --no-verify && git push --tags --no-verify"
  },
  "keywords": [
    "xls",
    "xlsx",
    "csv",
    "excel",
    "office",
    "spreadsheet",
    "workbook",
    "sheets"
  ],
  "dependencies": {
    "archiver": "^5.0.0",
    "dayjs": "^1.8.34",
    "fast-csv": "^4.3.1",
    "jszip": "^3.10.1",
    "readable-stream": "^3.6.0",
    "saxes": "^5.0.1",
    "tmp": "^0.2.0",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "chai": "^4.2.0",
    "chai-datetime": "^1.7.0",
    "chai-xml": "^0.3.2",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "express": "^4.16.4",
    "got": "^9.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.2.13",
    "mocha": "^7.2.0",
    "prettier-eslint": "^11.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "regenerator-runtime": "^0.13.7",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.1.0+sha512.c89847b0667ddab50396bbbd008a2a43cf3b581efd59cf5d9aa8923ea1fb4b8106c041d540d08acb095037594d73ebc51e1ec89ee40c88b30b8a66c0fae0ac1b",
  "pnpm": {
    "onlyBuiltDependencies": [
      "puppeteer",
      "husky",
      "esbuild"
    ]
  }
}
